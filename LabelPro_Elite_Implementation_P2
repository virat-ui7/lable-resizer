### 9. CURSOR CONFIGURATION FILE (.cursor)

```yaml
# .cursor - Configuration for Cursor AI
# This file tells Cursor how to understand your project

project:
  name: "LabelPro"
  description: "Label resizing SaaS for e-commerce sellers"
  version: "2.0"

structure:
  root: "src"
  components: "src/components"
  pages: "src/app"
  lib: "src/lib"
  server: "src/server"
  styles: "src/styles"
  types: "src/types"
  tests: "tests"

rules:
  # Components
  - pattern: "src/components/**/*.tsx"
    description: "React components - use 'use client' if needed client-side"
    rules:
      - "Export with named export + default"
      - "Add JSDoc comments for props"
      - "Include __tests__ folder for unit tests"
      - "Use TypeScript props interface"
  
  # Hooks
  - pattern: "src/lib/hooks/**/*.ts"
    description: "Custom React hooks"
    rules:
      - "Must start with 'use' prefix"
      - "Return tuple or object, not void"
      - "Include error handling"
  
  # API Routes
  - pattern: "src/app/api/**/*.ts"
    description: "Next.js API routes"
    rules:
      - "Export separate GET, POST, PUT, DELETE functions"
      - "Include error handling with 400/401/500"
      - "Validate auth before processing"
      - "Log all errors for debugging"
  
  # Database
  - pattern: "src/lib/supabase/**/*.ts"
    description: "Supabase client and queries"
    rules:
      - "Use server.ts for secret operations"
      - "Use client.ts for public operations"
      - "All queries must use RLS"
  
  # Styles
  - pattern: "src/styles/*.css"
    description: "Global CSS - use design system variables"
    rules:
      - "All colors must use CSS variables from design system"
      - "All spacing must use 8px units"
      - "No inline styles in components"

dependencies:
  critical:
    - "next@14"
    - "@supabase/supabase-js@2"
    - "react@18"
    - "tailwindcss@3"
    - "stripe@14"
  
  recommended:
    - "zustand" (state management)
    - "react-hook-form" (forms)
    - "zod" (validation)
    - "date-fns" (dates)
    - "next-auth" (alternative auth)

environment:
  required:
    - NEXT_PUBLIC_SUPABASE_URL
    - NEXT_PUBLIC_SUPABASE_ANON_KEY
    - SUPABASE_SERVICE_ROLE_KEY
    - NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY
    - STRIPE_SECRET_KEY
    - STRIPE_WEBHOOK_SECRET
    - DATABASE_URL

standards:
  code:
    - "Use TypeScript strict mode"
    - "No any types - always specific"
    - "Use const assertions for constants"
    - "Prefer functional components"
  
  naming:
    - "Components: PascalCase (Button.tsx)"
    - "Utilities: camelCase (cn.ts)"
    - "Constants: UPPER_SNAKE_CASE"
    - "Types: PascalCase (UserType.ts)"
  
  imports:
    - "Absolute imports from @/lib, @/components"
    - "Group imports: React â†’ external â†’ local"
    - "One import per line for clarity"

testing:
  framework: "Jest + React Testing Library"
  coverage:
    - "Components: 80% coverage minimum"
    - "Utilities: 90% coverage minimum"
    - "Critical paths: 100% coverage"
  
  pattern:
    - "Test file in __tests__ folder next to source"
    - "Name: ComponentName.test.tsx"
    - "Describe blocks by responsibility"

performance:
  targets:
    - "FCP: <2.5s"
    - "LCP: <2.5s"
    - "CLS: <0.1"
    - "Bundle size: <150KB"
  
  rules:
    - "Lazy load heavy components"
    - "Image optimization (next/image)"
    - "Code splitting by route"
    - "No inline large objects"

accessibility:
  wcag_level: "AA"
  requirements:
    - "All inputs have labels"
    - "All images have alt text"
    - "Keyboard navigation support"
    - "Color contrast 4.5:1"
    - "ARIA labels where needed"

database:
  provider: "Supabase (PostgreSQL)"
  
  tables:
    users:
      - id (UUID, PK)
      - email (unique)
      - subscription_tier (free|pro|enterprise)
      - created_at
    
    labels:
      - id (UUID, PK)
      - name (unique)
      - width_mm, height_mm
      - category (amazon_fba|walmart_wfs|etc)
    
    label_designs:
      - id (UUID, PK)
      - user_id (FK)
      - label_base_id (FK)
      - elements (JSONB)
  
  security:
    - "Enable RLS on all tables"
    - "User can only see own data"
    - "Labels table is public read"

prompts:
  core:
    - "Create component with TypeScript props interface"
    - "Add error boundaries and loading states"
    - "Use design system (spacing, colors)"
    - "Include accessibility (labels, ARIA)"
  
  api:
    - "Validate input with Zod"
    - "Check auth status first"
    - "Return proper HTTP codes"
    - "Log errors for debugging"
  
  database:
    - "Always use parameterized queries"
    - "Include RLS in queries"
    - "Handle errors gracefully"
```

